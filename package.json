{
  "name": "cel-ts",
  "version": "0.1.0-alpha",
  "description": " Yet another CEL interpreter for TypeScript",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist", "LICENSE", "README.md"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "lint": "biome check --diagnostic-level=error .",
    "lint:fix": "biome check --diagnostic-level=error --write .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "conformance": "bun run test/conformance/run.ts",
    "conformance:serve": "bun run test/conformance/run.ts --serve",
    "benchmark": "bun run test/benchmark/run.ts",
    "profile:build": "pnpm run build && pnpm exec tsc -p test/profile/tsconfig.json",
    "profile:serve": "pnpm run profile:build && pnpm exec clinic flame --dest test/profile/.clinic -- node test/profile/run.js",
    "version": "pnpm run build",
    "clean": "rm -rf dist coverage docs/api",
    "docs": "pnpm run docs:build",
    "docs:build": "pnpm exec typedoc",
    "docs:watch": "pnpm exec typedoc --watch",
    "docs:view": "pnpm run docs:build && (open docs/api/index.html || xdg-open docs/api/index.html)",
    "docs:serve": "pnpm run docs:view"
  },
  "keywords": [
    "cel",
    "common-expression-language",
    "expression",
    "parser",
    "evaluator",
    "interpreter",
    "google-cel",
    "policy",
    "rules-engine",
    "dsl"
  ],
  "author": "",
  "license": "Apache-2.0",
  "homepage": "https://github.com/taichimaeda/cel-ts/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taichimaeda/cel-ts.git"
  },
  "bugs": {
    "url": "https://github.com/taichimaeda/cel-ts/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.14.2",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@marcbachmann/cel-js": "6.4.0",
    "allure-commandline": "2.30.0",
    "allure-js-commons": "3.0.0",
    "bun-types": "1.1.34",
    "cel-js": "0.8.1",
    "chevrotain": "11.0.3",
    "cli-table3": "0.6.5",
    "clinic": "13.0.0",
    "consola": "3.2.3",
    "fast-check": "4.4.0",
    "mitata": "^1.0.34",
    "protobufjs": "^8.0.0",
    "tsup": "8.3.5",
    "typedoc": "0.26.11",
    "typescript": "5.6.3"
  },
  "dependencies": {
    "antlr4": "4.13.2"
  }
}
