{
  "name": "cel-ts",
  "version": "0.1.0-alpha",
  "description": " Yet another CEL interpreter for TypeScript",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "conformance": "bun run test/conformance/run.ts",
    "conformance:serve": "bun run test/conformance/run.ts --serve",
    "benchmark": "bun run test/benchmark/run.ts",
    "prepublishOnly": "pnpm run lint && pnpm run typecheck && pnpm run test && pnpm run build",
    "version": "pnpm run build",
    "clean": "rm -rf dist coverage docs/api",
    "docs": "pnpm run docs:serve",
    "docs:build": "pnpm exec typedoc",
    "docs:watch": "pnpm exec typedoc --watch",
    "docs:serve": "concurrently -k -s first \"pnpm run docs:watch\" \"python3 -m http.server --directory docs/api 8000\""
  },
  "keywords": [
    "cel",
    "common-expression-language",
    "expression",
    "parser",
    "evaluator",
    "interpreter",
    "google-cel",
    "policy",
    "rules-engine",
    "dsl"
  ],
  "author": "",
  "license": "Apache-2.0",
  "homepage": "https://github.com/taichimaeda/cel-ts/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taichimaeda/cel-ts.git"
  },
  "bugs": {
    "url": "https://github.com/taichimaeda/cel-ts/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.14.2",
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "allure-commandline": "2.36.0",
    "allure-js-commons": "3.4.3",
    "bun-types": "1.3.5",
    "cli-table3": "0.6.5",
    "concurrently": "9.2.1",
    "consola": "3.4.2",
    "fast-check": "4.5.2",
    "mitata": "^1.0.34",
    "protobufjs": "^8.0.0",
    "tsup": "8.5.1",
    "typedoc": "0.28.15",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "antlr4": "4.13.2"
  }
}